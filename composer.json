{
    "name": "tochka-developers/jsonrpc",
    "description": "JsonRpc extension for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Babushkin",
            "email": "darkdarin@tochka.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "illuminate/support": "5.7.*|5.8.*|^6.0|^7",
        "illuminate/http": "5.7.*|5.8.*|^6.0|^7",
        "illuminate/container": "5.7.*|5.8.*|^6.0|^7",
        "illuminate/pipeline": "5.7.*|5.8.*|^6.0|^7",
        "phpdocumentor/reflection-docblock": "~4.3|^5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\JsonRpc\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tochka\\JsonRpc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tochka\\JsonRpc\\JsonRpcServiceProvider"
            ]
        }
    }
}
